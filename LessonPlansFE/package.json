    "@mui/material": "^5.15.18",
    "@react-pdf/renderer": "^3.4.4",
    "@reduxjs/toolkit": "^2.2.4",
    "@tinymce/tinymce-react": "^5.0.1",
    "axios": "^1.6.8",
    "dompurify": "^3.1.5",
    "html-react-parser": "^5.1.10",
    "quill": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "uuid": "^9.0.1"
